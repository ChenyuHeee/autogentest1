# Rename this file to .env and provide real credentials before running the workflow.
DEEPSEEK_API_KEY=your_deepseek_api_key_here
DEEPSEEK_BASE_URL=https://api.deepseek.com/v1
DATA_PROVIDER=yfinance
DATA_MODE=hybrid
# Optional: override default model name
# DEEPSEEK_MODEL=deepseek-chat
ALPHAVANTAGE_API_KEY=optional_alpha_vantage_key
NEWS_API_KEY=optional_news_api_key
LOG_LEVEL=INFO
MAX_POSITION_OZ=5000
STRESS_VAR_MILLIONS=3.0
DAILY_DRAWDOWN_PCT=3.0
DEFAULT_POSITION_OZ=1200
PNL_TODAY_MILLIONS=0.4
LOCAL_MODEL_ENABLED=false
LOCAL_MODEL_NAME=qwen2.5:14b-instruct-q4_K_M
LOCAL_MODEL_BASE_URL=http://127.0.0.1:11434/v1
# 如果需要认证，请提供 API Key；默认本地服务无需填写
LOCAL_MODEL_API_KEY=
# 指定使用本地模型的代理名称，逗号分隔，例如："DataAgent,MacroAnalystAgent,QuantResearchAgent"
LOCAL_MODEL_AGENTS=
# 启用代码执行能力（Quant/Tech 默认开启）
CODE_EXECUTION_ENABLED=true
CODE_EXECUTION_AGENTS=QuantResearchAgent,TechAnalystAgent
CODE_EXECUTION_TIMEOUT=90
WORKFLOW_FORMAT_RETRY_LIMIT=2
WORKFLOW_MAX_PLAN_RETRIES=3
MARKET_DATA_MAX_AGE_MINUTES=1440
MARKET_DATA_CACHE_MINUTES=10
MARKET_DATA_RETRY_TOTAL=4
MARKET_DATA_RETRY_BACKOFF=1.0
NEWS_WATCHER_ENABLED=false
NEWS_WATCHER_POLL_SECONDS=300
NEWS_WATCHER_KEYWORDS=war,fed,rate,cpi
NEWS_WATCHER_VOL_THRESHOLD=0.4
